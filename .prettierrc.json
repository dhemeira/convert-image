{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 100,
  "trailingComma": "es5",
  "singleAttributePerLine": true,
  "endOfLine": "crlf",
  "bracketSameLine": true,
  "quoteProps": "consistent",
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "ignore"
}
